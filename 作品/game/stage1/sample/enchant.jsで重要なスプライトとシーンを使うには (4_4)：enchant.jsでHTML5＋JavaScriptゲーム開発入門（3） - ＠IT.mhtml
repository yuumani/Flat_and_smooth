@charset "shift_jis";

#COIDP { display: none; }

#g_nav { --base_w:990px; --base_h:80px; font-family: -apple-system, sans-serif, HiraKakuProN-W3, Meiryo, メイリオ, ArialMT, "Hiragino Kaku Gothic Pro", "ヒラギノ角ゴ Pro W3", Osaka, Verdana, "ＭＳ Ｐゴシック"; text-align: left; }

#g_nav a:hover { text-decoration: none; }

#g_nav_o { width: 100%; background: rgb(255, 255, 255); border-bottom: 1px solid rgb(221, 221, 221); box-sizing: border-box; }

#g_nav[data-theme="dark"] #g_nav_o { background: rgb(0, 0, 0); }

#g_nav_i { display: flex; position: relative; width: var(--base_w); height: var(--base_h); margin: 0px auto; }

#g_nav .colBox.colBoxWarning, #g_nav .colBox.colBoxWarning * { all: initial; }

#g_nav .colBox.colBoxWarning { display: block; background: rgb(238, 238, 238); }

#g_nav[data-theme="dark"] .colBox.colBoxWarning { background: rgb(34, 34, 34); }

#g_nav .colBox.colBoxWarning .colBoxOuter { display: block; width: var(--base_w); margin: 0px auto; padding: 4px 5px 5px; }

#g_nav .colBox.colBoxWarning li::before { content: "・"; font-size: 12px; color: rgb(51, 51, 51); }

#g_nav[data-theme="dark"] .colBox.colBoxWarning li::before { color: rgb(204, 204, 204); }

#g_nav .colBox.colBoxWarning li a { font-size: 12px; cursor: pointer; color: rgb(51, 51, 51); }

#g_nav .colBox.colBoxWarning li a:hover { color: rgb(0, 0, 0); text-decoration: underline; }

#g_nav[data-theme="dark"] .colBox.colBoxWarning li a { color: rgb(204, 204, 204); }

#g_nav[data-theme="dark"] .colBox.colBoxWarning li a:hover { color: rgb(238, 238, 238); }

.g_nav_box { display: flex; align-items: center; height: var(--base_h); }

._flex { flex: 1 1 0%; }

.g_btn { width: 80px; height: var(--base_h); padding: 10px 0px 0px; box-sizing: border-box; }

.g_img { width: 20px; height: 20px; margin: 0px auto; }

.g_txt { margin-top: 5px; font-size: 11px; line-height: 11px; text-align: center; color: rgb(102, 102, 102); }

#g_nav[data-theme="dark"] .g_txt { color: rgb(204, 204, 204); }

.g_inbtn { position: relative; display: block; width: 70px; height: 60px; text-decoration: none; margin: 0px auto; padding: 13px 5px 5px; box-sizing: border-box; cursor: pointer; background: transparent; border: 1px solid transparent; transition-property: background, border; transition-duration: 0.2s; z-index: 999; }

.g_inbtn:hover { background: rgb(238, 238, 238); border: 1px solid rgb(204, 204, 204); }

#g_nav[data-theme="dark"] .g_inbtn:hover { background: rgb(34, 34, 34); border: 1px solid rgb(17, 17, 17); }

.g_btn[data-menu="0"] { z-index: 900; }

.g_float { position: absolute; top: 10px; width: calc(100% - 10px); height: 0px; overflow: hidden; background: transparent; box-sizing: border-box; z-index: 998; transition-property: height; transition-duration: 0.3s; }

.g_float_left { left: 5px; }

.g_float_right { right: 5px; }

.g_btn[data-menu="1"] .g_float_left { height: 640px; }

.g_btn[data-menu="1"] .g_float_right { min-height: 130px; height: auto; }

.g_lines { position: relative; margin: 0px auto; width: 20px; height: 19px; }

.g_line { position: absolute; left: 0px; width: 100%; height: 3px; margin: 0px auto; background: rgb(102, 102, 102); border: 0px; transform: rotate(0deg); transition-property: transform, top, bottom, width, height, background, border; transition-duration: 0.5s; }

#g_nav[data-theme="dark"] .g_line { background: rgb(153, 153, 153); }

.g_line:nth-child(1) { top: 0px; }

.g_line:nth-child(2) { top: 8px; }

.g_line:nth-child(3) { top: 8px; }

.g_line:nth-child(4) { bottom: 0px; }

#g_btn_menu[data-menu="1"] .g_line:nth-child(1) { width: 0px; }

#g_btn_menu[data-menu="1"] .g_line:nth-child(2) { transform: rotate(-45deg); }

#g_btn_menu[data-menu="1"] .g_line:nth-child(3) { transform: rotate(45deg); }

#g_btn_menu[data-menu="1"] .g_line:nth-child(4) { width: 0px; }

.g_search_icons { position: relative; margin: 0px auto; width: 20px; height: 19px; }

.g_search_icon { position: absolute; box-sizing: border-box; overflow: hidden; transition-property: transform, top, left, margin, bottom, width, height, background, border; transition-duration: 0.5s; }

#g_nav[data-theme="dark"] .g_search_icon { background: rgb(153, 153, 153); }

.g_search_icon:nth-child(1) { top: -6px; left: -5px; width: 20px; height: 20px; border: 5px solid rgb(170, 170, 170); border-radius: 40px; }

.g_search_icon:nth-child(2) { top: 12px; left: 8px; width: 12px; height: 5px; margin: 0px auto; background: rgb(170, 170, 170); border: 0px; transform: rotate(45deg); transition-property: transform, top, bottom, width, height, background, border; transition-duration: 0.5s; }

#g_btn_search[data-menu="1"] .g_search_icon:nth-child(1) { top: 8px; left: 0px; width: 100%; height: 3px; margin: auto; transform: rotate(45deg); background: rgb(102, 102, 102); border: 0px solid transparent; }

#g_btn_search[data-menu="1"] .g_search_icon:nth-child(2) { top: 8px; left: 0px; width: 100%; height: 3px; margin: auto; transform: rotate(-45deg); background: rgb(102, 102, 102); }

.g_media { display: flex; flex-wrap: wrap; align-items: flex-start; place-content: flex-start space-between; width: 100%; padding: 60px 5px 5px; background: rgb(255, 255, 255); border: 1px solid rgb(204, 204, 204); box-sizing: border-box; }

.g_media_list { width: calc(20% - 10px); height: 85px; margin-top: 10px; text-decoration: none; color: rgb(51, 51, 51); background: transparent; border: 1px solid transparent; box-sizing: border-box; }

.g_media_empty { width: calc(20% - 10px); }

.g_media_img { height: 60px; background-repeat: no-repeat; background-position: center center; background-size: contain; }

.g_media_name { font-size: 11px; text-align: center; height: 20px; color: rgb(102, 102, 102); }

.g_media_list:hover { border: 1px solid rgb(221, 221, 221); background: rgb(238, 238, 238); }

.g_media_list:hover .g_media_img { filter: brightness(120%); }

.g_search_box { width: 100%; padding: 60px 5px 5px; background: rgb(255, 255, 255); border: 1px solid rgb(204, 204, 204); box-sizing: border-box; }

